.combo_package_list {
  width: 100%;
}

.combo_package_content .combo_package_name {
  padding: 8px 10px;
  background: #f7f7f7;
  color: #666;
}

.label {
  cursor: default;
  color: #666;
}

.span{
  color: #666;
}

.combo_package_content .combo_package_name .label.checkbox checkbox {
  display: none;
}

.combo_package_content .combo_package_name .label.checkbox image {
  width: 15px;
  height: 15px;
  display: inline-block;
  background: #FFF;
  color: #FFF;
  position: relative;
  top: 2px;
  right: 3px;
}

.combo_package_name .span.span-right{
  float: right;
}

.price, #package_price .b{
  color: #ff0000 !important;
}

.combo_package_content .goods_info {
  width: 94%;
  overflow: hidden;
  border-bottom: 1px solid #f8f8f8;
  background: #fff;
  padding: 8px 3%;
}

.combo_package_content .goods_info .goods_img {
  display: block;
  width: 30%;
  text-align: center;
  line-height: 100%;
  float: left;
}

.combo_package_content .goods_info .goods_img .a {
  display: inline-block;
  width: 100%;
  padding: 50% 0;
  text-align: center;
  position: relative;
}

.combo_package_content .goods_info .goods_img .a .pic {
  max-height: 100%;
  max-width: 100%;
  vertical-align: middle;
  position: absolute;
  top: 0;
  left: 0;
}

.combo_package_content .goods_info .data_info {
  display: block;
  width: 66%;
  float: left;
  position: relative;
  margin-left: 4%;
}

.combo_package_content .goods_info .data_info .goods_name {
  width: 100%;
  height: 40px;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  line-height: 150%;
  color: #555;
}

.combo_package_content .goods_info .data_info .price_share {
  width: 100%;
  height: 25px;
  line-height: 25px;
  bottom: 25px;
}

.combo_package_content .goods_info .data_info .price_share .price {
  display: block;
  float: left;
  color: red;
}

.combo_package_content .goods_info .data_info .select_sku {
  color: #999;
  font-size: 13px;
} 

.combo_package_content .goods_info .data_info view.select_sku_28 {
  color: #999;
  font-size: 13px;
}

.fa {
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.fa-lg {
  font-size: 1.33333333em;
  line-height: .75em;
  vertical-align: -15%;
}

.combo_package_content .goods_info .data_info .select_sku .i {
  float: right;
  position: relative;
  top: 2px;
}

.fa-angle-right:before {
  content: '>';
  font-size: 13px;
}

.combo_package_buttom {
  width: 100%;
  height: 50px;
  background: #fff;
  position: fixed;
  bottom: 0;
}

.combo_package_buttom .combo_package_info {
  width: 65%;
  float: left;
}

.combo_package_buttom .combo_package_info .p {
  height: 25px;
  color: #666;
  padding-left: 10px;
  font-size: 12px;
}

.combo_package_buttom .combo_package_info .p .package_price {
  margin-top: 5px;
  display: inline-block;
  font-size: 13px;
}

.combo_package_buttom .combo_package_info .p .package_price .b {
  color: #ff0000;
}

.combo_package_buttom .combo_package_info .p .package_price .b .span{
  color: #ff0000;
}

#package_price{
  font-size: 13px;
}

.combo_package_buttom .combo_package_info .p .original_price {
  white-space: nowrap;
  font-size: 12px;
}

.s, .strike, .del {
  display: inline;
  text-decoration: line-through;
  font-size: 12px;
}

.s .span{
  font-size: 12px;
}

.combo_package_buttom .combo_package_info .p .save_the_price {
  margin-left: 10px;
  display: inline-block;
  font-size: 12px;
}

.combo_package_buttom .combo_package_info .p .save_the_price .span {
  font-size: 12px;
}

.combo_package_buttom .btn-jiesuan, .combo_package_buttom .btn-jiesuan-disabled {
  width: 35%;
  line-height: 50px;
  text-align: center;
  color: #fff;
  background: #FF4E00;
  float: right;
}

.blank_view{
  width: 100%;
  height: 50px;
}

.combo_package_content {
  background: #fff;
}

.combo_package_list .divider-line {
  height: 13px;
  background: #eee;
  margin: 0;
  border: 0;
}

.mask-div{
  background-color: #000;
  opacity: 0.4;
  height: 100%;
  margin: 0px;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 111;
  display: none;
}


.s-buy{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  border-top: 1px solid #f0f0f0;
  box-shadow: 0 -3px 3px rgba(0, 0, 0, .3);
  z-index: 501;
  font-family: Helvetica, "STHeiti STXihei", "Microsoft YaHei", Tohoma, Arial;
  background: #f7f7f7;
}

.js-thumbnail{
  height: 58px;
}

.popup{
  border-radius: 3px;
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -20px;
  margin-left: -75px;
  width: 150px;
  padding: 5px 10px;
  background: rgba(0, 0, 0, .6);
  color: #fff;
  text-align: center;
  z-index: 999;
}

.s-buy-pro{
  overflow: hidden;
  padding: 10px;
  background: #fff;
}

.s-buy-pro .pro-img{
  float: left;
  width: 23%;
  display: block;
  overflow: hidden;
}

.s-buy-pro .pro-img image{
  width: 58px;
}

.s-buy-pro .pro-info{
  float: left;
  width: 60%;
  font-size: 14px;
  overflow: hidden;
  line-height: 1.5;
}

.s-buy-pro .pro-price{
  color: #F44 !important;
  font-size: 16px;
}

.s-buy-pro .icon-close{
  float: right;
  filter: alpha(opacity = 30);
  height: 22px;
  width: 22px;
  background-image: url(https://b.yzcdn.cn/v2/image/wap/sku/icon_close.png);
  background-size: 22px 22px;
}

.s-buy-ul.specification{
  padding: 5px 0;
  max-height: 260px;
  background-color: #FFFFFF;
  -webkit-overflow-scrolling: touch;
  overflow-y: scroll;
  z-index: 9000;
}

.s-buy-li{
  overflow: hidden;
  padding: 10px 10px 5px 10px;
  font-size: 12px;
  color: #999;
  background-color: #FFFFFF;
}

.s-buy-li .left{
  white-space: nowrap;
  font-size: 14px;
  color: #555;
  margin: 7.5px 0 7.5px 0;
  /* float: left; */
}

.s-buy-li .right{
  overflow: hidden;
}

.quantity-minus-disabled{
  background-color: #f8f8f8 !important;
  border-color: #e8e8e8 !important;
  color: #bbb !important;
}

.s-buy-li .right .reduce, .s-buy-li .right .add{
  float: left;
  height: 34px;
  line-height: 34px;
  width: 40px;
  margin: 0;
  border: 1px solid #e4e4e4;
  background: #fff;
  text-align: center;
  color: #666;
  font-size: 18px;
  font-weight: 400;
}

.s-buy-li .right .number{
  float: left;
  text-align: center;
  line-height: 34px;
  max-width: 33px;
  padding: 0 5px;
  margin: 0;
  border-top: 1px solid #e4e4e4;
  border-bottom: 1px solid #e4e4e4;
  border-left: 0;
  border-right: 0;
  background: #fff;
  color: #000;
  font-size: 15px;
  height: 34px;
}

.s-buy-li .last-goods{
  display: inline-block;
  margin-top: 5px;
  color: #333;
}

.btn_wrap .btn, .s-buy .btn{
  float: left;
  height: 50px;
  line-height: 50px;
  text-decoration: none;
  display: block;
  text-align: center;
  font-size: 16px;
  color: #FFF;
  width: 21%;
  cursor: pointer;
  background: #fff;
}

.s-buy-btn{
  background: #fff;
  overflow: hidden;
}

.tip-box{
  width: 100%;
  margin: 10% auto;
  text-align: center;
}

.tip-box .tip-icon{
  width: 135px;
  height: 93px;
  margin: 0 auto;
}

.tip-box .tip-text{
  color: #666;
  line-height: 20px;
  padding: 10px;
}

.bottom-fix{
  border: 0;
  bottom: -1px;
  padding: 0;
  display: block;
  z-index: 100;
  position: fixed;
  left: 0;
  width: 100%;
