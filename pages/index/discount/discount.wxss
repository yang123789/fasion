.custom-tag-list{
  padding-top: 0;
  margin-top: 0;
  margin-bottom: 10px;
  height: 100%;
  background-color: #f3f5f7;
  overflow: hidden;
  position: relative;
  z-index: 0;
}

.group-list-box{
  width: 100%;
  margin: 10px 0;
  float: left;
  overflow-y: auto;
  height: 100%;
}

.slick{
  margin-bottom: 10px;
  max-height: 200px;
}

.slick .a{
  width: 100%;
}

.slick image{
  width: 100%;
}

.group-list{
  overflow: hidden;
}

.group-list-box .group-list .li{
  width: 43%;
  float: left;
  margin: 0 1% 5px 1%;
  display: block;
  position: relative;
  background: #fff;
  padding: 0 2.5%;
  border-top: 0.05rem solid #eee;
  border-bottom: 0.05rem solid #eee;
}

.group-list-box .group-list .li .p-img{
  position: relative;
  width: 100%;
  min-height: 70px;
  overflow: hidden;
  margin: 0 auto;
  text-align: center;
  cursor: pointer;
}

.group-list-box .group-list .li .p-img .a{
  display: block;
  padding: 50% 0;
  height: 0;
  width: 100%;
}

.group-list-box .group-list .li .p-img .a image{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.group-list-box .group-list .li .p-img .brand-time{
  width: 100%;
  height: 35px;
  line-height: 35px;
  color: #999;
  margin-top: 12px;
  font-family: verdana;
  font-size: 12px;
  position: absolute;
  left: 0px;
  bottom: 0px;
  background: rgba(0, 0, 0, .4);
  text-align: left;
}

.group-list-box .group-list .li .p-img .brand-time .i{
  width: 23px;
  height: 23px;
  vertical-align: middle;
  display: inline-block;
}

.group-list-box .group-list .li .p-img .brand-time .span{
  color: #fff;
  font-size: 0.73rem;
  vertical-align: middle;
}

.group-list-box .group-list .li .brand-name{
  display: block;
  width: 100%;
  height: 1.5rem;
  line-height: 1.5rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #222;
  font-size: 14px;
  text-align: center;
}

.group-list-box .group-list .li .brand-info{
  height: initial;
}

.group-list-box .group-list .li .brand-info .brand-info-left{
  float: none;
}

.brand-info .brand-info-left .b-price{
  color: orange;
  font-weight: bold;
  font-size: 12px;
  line-height: 29px;
  float: left;
  margin: 5px 0;
}

.brand-info .brand-info-left .b-price .em{
  display: inline-block;
  text-indent: 5px;
  font-size: 0.73rem;
  color:orange;
  margin-left:-6px;
}

.brand-info .brand-info-left .buyer{
  font-size: 0.68rem;
  color: #999;
  float: right;
  line-height: 29px;
  text-decoration-line: line-through;
  margin:5px 0;
}

.none-goods{
  color: #939393;
  text-align: center;
  margin-top: 100px;
}

.none-goods image{
  margin-bottom: 20px;
  width: 60px;
  height: 58.6364px;
}