.return-process{
  background: #f7f7f7;
  border-bottom: 1px dashed #ccc;
}

.return-process .ul{
  list-style: none;
  margin: 0 auto 20px auto;
  width: 300px;
}

.clear{
  clear: both;
  zoom: 1;
}

.clear:before, .clear:after{
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.return-process .li{
  float: left;
  margin-top: 20px;
}

.return-process .name{
  margin: 0;
  color: #999;
  font-size: 12px;
  text-align: center;
}

.return-process .step01 .name{
  text-align: left;
}

.return-process .current .name{
  color: #f37872;
}

.return-process .number{
  display: inline-block;
  width: 20px;
  height: 20px;
  line-height: 20px;
  border-radius: 50%;
  background: #ccc;
  color: #fff;
  font-weight: bold;
  text-align: center;
  vertical-align: middle;
}

.return-process .current .number, .return-process .current .prcess-line{
  background: #f37872;
}

.return-process .prcess-line{
  display: inline-block;
  height: 2px;
  width: 60px;
  background: #ccc;
  vertical-align: middle;
}

.return-process .prcess-line-r{
  margin-left: 0px;
}

.return-process .prcess-line-l{
  margin-right: 0px;
}

.return-process .step03 .name {
  text-align: right;
}

.clear:after{
  content: ' ';
  height: 0;
  display: block;
  visibility: hidden;
  clear: both;
}

.form-c{
  margin: 20px 0 0 0;
  display: block;
}

.show{
  display: block !important;
}

#form1{
  margin: 0;
}

.row {
  margin: 15px 0 0 0;
  font-size: 16px;
}

.form-c .row {
  margin: 15px 0 0 0;
  font-size: 16px;
}

#form1 .row {
  margin: 0px;
  border-bottom: 1px solid #f1f1f1;
  background-color: #fff;
}

.columns{
  overflow: hidden;
}

.columns .label{
  width: 24%;
  float: left;
  font-size: 12px;
  text-align: center;
  margin-bottom: 0px;
  line-height: 40px;
}

.color-red{
  margin-right: 5px;
  color: red;
}

.columns .columns-select{
  width: 72%;
  float: right;
  font-size: 12px;
  position: relative;
}

.columns-select:after{
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #C8C8CD;
  border-style: solid;
  position: absolute;
  top: 50%;
  right: 15px;
  margin-top: -3px;
}

textarea{
  border: none;
  font-size: 12px;
  width: 94%;
