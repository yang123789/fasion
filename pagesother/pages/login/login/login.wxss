page {
  background: #fff;
}

.nk_logo {
  text-align: center;
}

.nk_logo image {
  width: 100%;
  max-height: 190px;
}

image {
  vertical-align: top;
}

.nk_logo .back-home {
  position: absolute;
  z-index: 80;
  width: 30px;
  height: 25px;
  line-height: 25px;
  background: #fff;
  top: 15px;
  padding-right: 8px;
  border-radius: 18px;
  text-align: center;
}

.nk_logo .back-home .span.data-home {
  height: 20px;
  width: 20px;
  display: block;
  margin: 2px 0 0 8px;
}

.nk_top {
  line-height: 40px;
  text-align: center;
}

.nk_top .nk_cell {
  display: inline-block;
  width: 50%;
  text-align: center;
  background: #fff;
  color: gray;
  font-size: 14px;
}

.nk_top .nk_cell.active .span, .nk_top .nk_cell_one.active .span {
  color: #ff4e05;
  border-bottom: 1px solid #ff4e05;
}

.nk_top .nk_cell span, .nk_top .nk_cell_one span {
  height: 35px;
  display: inline-block;
  padding: 0 5px;
}

.log-wp {
  padding: 0 10px;
  background: #fff;
}

.log-box {
  width: 100%;
}

.log-box .log-cont {
  position: relative;
  display: block;
  height: 45px;
  width: 80%;
  margin-left: 10%;
  border-bottom: #e9ecee 1px solid;
  background: #fff;
}

.log-box .log-cont label {
  position: absolute;
  left: 0;
  width: 100%;
}

.log-box .log-cont label .span.username, .log-box .log-cont label .span.password {
  width: 30px;
  height: 30px;
  text-indent: -9999px;
  vertical-align: middle;
  display: inline-block;
  position: relative;
  top: -3px;
}

.log-box .log-cont label input {
  height: 35px;
  font-size: 14px;
  color: #34495e;
  border: none;
  padding: 5px 1px;
  width: 80%;
  margin-left: 3%;
}

.lang-btn {
  display: inline-block;
  position: relative;
  vertical-align: middle;
  cursor: pointer;
  white-space: nowrap;
  background-color: #ff4e00;
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  color: #fff;
  border: none;
  letter-spacing: 1px;
  overflow: hidden;
  text-align: center;
  border-radius: 18px;
  width: 80%;
  margin-left: 10%;
}

.register_immediately {
  background: #fff;
  color: #ff4e00;
  height: 33px;
  line-height: 33px;
  border: 1px solid #ff4e00;
}

.log-box .lang-btn:first-child {
  margin-top: 30px;
}

.log-box .lang-btn {
  margin-top: 1rem;
  width: 80%;
  margin-left: 10%;
}

.log-wp .msg {
  width: 80%;
  margin: 10px auto 0 auto;
  text-align: right;
}

.log-wp .msg .a {
  font-size: 12px;
  color: #f34048;
}

.footer {
  min-height: 86px;
  padding: 0;
  text-align: center;
  line-height: 16px;
  background: #fff;
  margin: 0;
}

.copyright {
  background: #fff;
  border-bottom: none;
}

.nk-cont {
  position: relative;
  display: block;
  height: 45px;
  border-bottom: #e9ecee 1px solid;
  margin-left: 10%;
  background: #fff;
  width: 80%;
  font-size: 14px;
}

.nk-cont label {
  line-height: 45px;
}

.nk-cont input {
  height: 35px;
  font-size: 12px;
  color: #34495e;
  border: none;
  padding: 5px 1px;
  width: 6rem;
  margin-left: 3%;
}

.verify {
  width: 30%;
  float: right;
  height: 45px;
  line-height: 45px;
  padding-right: 7px;
  position: relative;
}

.verify image{
  width: 100%;
  max-height: 60px;
  vertical-align: middle;
}

#sendOutCode {
  border: 1px solid #ff4e00;
  padding: 2px 3px;
  line-height: 1.5;
  font-size: 12px;
  color: #ff4e00;
  height: 25px;
  font-weight: bold;
  float: right;
  margin: 10px 4px 4px 4px;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  width: 5rem;
  text-align: center;
  background: #fff;
  border-radius: 12px;
}

.findback {
  text-align: center;
  background: #FFF;
  position: fixed;
  top: 35%;
  width: 80%;
  margin-left: 10%;
  border-radius: 10px;
  overflow: hidden;
  opacity: 0;
  z-index: 99999;
}

.findback .a {
  display: inline-block;
  padding: 10% 8%;
  width: 32%;
  color: black;
  font-size: 17px;
}

.findback image {
  position: relative;
  width: 70%!important;
}

.findback .p {
  height: 30px;
  line-height: 30px;
  overflow: hidden;
  margin-top: 5px;
  font-size: 14px;
}

#mask-layer-login {
  position: fixed;
  top: 0px;
  width: 100%;
  height: 100%;
  z-index: 9999;
  background: rgba(0, 0, 0, 0.75);
}