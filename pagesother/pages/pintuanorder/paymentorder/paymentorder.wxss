.noscroll {
  height: 100%;
  overflow: hidden;
}

.hr{
  position: fixed;
  top: 0;
  width: 100%;
  height: 1px;
  background: #CCC;
  z-index:10;
}

.order{
  background: #ffffff;
}

.clearfix{
  zoom: 1;
}

.order-goods-item{
  padding: 0 10px 10px;
  background-color: #fafafa;
}

.order-goods-item{
  margin-top: 10px;
}

.order-goods-item.first{
  margin-top: 0;
}

.name-card{
  margin-left: 0px;
  width: auto;
  padding: 5px 0;
  overflow: hidden;
  position: relative;
  border-bottom: 0 !important;
}

.order-goods-item .name-card .thumb{
  width: 66px;
  height: 66px;
  float: left;
  position: relative;
  margin-left: auto;
  margin-right: auto;
  overflow: hidden;
  background-size: cover;
}

.name-card .thumb image{
  position: absolute;
  margin: 0 auto;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: auto;
  height: auto;
  max-width: 100%;
  max-height: 100%;
}

.order-goods-item .name-card .detail{
  margin-left: 80px;
  min-height: 38px;
  width: auto;
  position: relative;
}

.order-goods-item .name-card .detail-row{
  overflow: hidden;
  min-height: 20px;
  margin-bottom: 3px;
}

.order-goods-item .name-card .right-col{
  float: right;
}

.order-goods-item .name-card .left-col{
  padding-right: 50px;
}

.h3{
  display: block;
  font-size: 100%;
  font-family: inherit;
  margin: 0;
  padding: 0;
  -webkit-margin-start: 0px;
  -webkit-margin-end: 0px;
  font-weight: bold;
}

.left-col .h3{
  font-weight: normal;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}


.left-col .h3 .a{
  font-weight: normal;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.clearfix:after{
  content: '';
  display: table;
  clear: both;
}

.c-gray-darker{
  color: #666 !important;
}

.left-col .p.c-gray-darker{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.item-options{
  border-bottom: 1px solid #F2F2F2;
  overflow: hidden;
  line-height: 45px;
  padding: 0 10px;
}

.first-child{
  line-height: 30px;
}

.item-options .label{
  float: left;
  color: #333333;
}

.label .hit{
  font-size: 12px !important;
  color: #FF9800;
  font-weight: bold;
}

#user_telephone{
  float: right;
  text-align: right;
  height: 45px;
  line-height: 45px;
}

.item-options .span{
  float: right;
  font-size: 14px;
}

.color-gray{
  color: #9A9A9A;
}

.arrow-right:after{
  content: '>';
  background-size: 6px;
  width: 6px;
  height: 45px;
  line-height: 45px;
  display: block;
  margin-left: 5px;
  float: right;
}

.account_balance{
  color: #ff4000;
  font-size: 12px !important;
}

#account_balance, #use_point {
  width: 60px;
  border: 0;
  border-bottom: 1px solid #e3e3e3;
  line-height: 10px;
  text-align: center;
  color: #999;
}

.item-options .label .invoice-info{
  font-size: 12px;
  color: #FF9800;
  font-weight: bold;
}

.content-box {
  width: 100%;
  overflow: hidden;
  position:relative;
}

page {
  max-width: 100%;
}

.item-options.invoice {
  position: relative;
}

#leavemessage, #invoice-title, #taxpayer-identification-number{
  font-family: Helvetica, "STHeiti STXihei", "Microsoft YaHei", Tohoma, Arial;
  color: #666;
  height: 50px;
  padding: 13px 10px;
  border: 0;
  float: right;
  width: 65%;
  text-align: right;
  resize: none !important;
  outline:none !important;
  background: transparent;
  transition: height 0.5s;
  line-height: 16px;
}

.order-list{
  color: #333333;
  margin: 0 10px;
}

.h3.order-settlement{
  font-weight: normal;
  font-size: 14px;
  padding: 10px;
  border-bottom: 1px solid #F2F2F2;
}

.order-list .p{
  padding: 5px 0;
}

.orange-bold{
  color: #ff4000;
}

.order-list .p .span{
  float: right;
}

.bottom-fix{
  border: 0;
  display: block;
  z-index: 100;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  text-align: right;
  padding: 6px 7px;
  border-top: 1px solid #e5e5e5;
  background-color: #fff;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 0;
  border-top-width: 1px;
}

.order-total-pay, .order-total-pay .pay-container{
  padding: 0;
}

.pay-container .b{
  font-size: 14px;
}

.order-total-pay{
  line-height: 50px;
}

.font-size-12{
  font-size: 12px !important;
}

.font-size-12 view{
  font-size: 12px !important;
}

.pull-right{
  float: right;
}

.em{
  display: inline-block;
}

.theme-price-color{
  color: #F44 !important;
}

.font-size-13{
  font-size: 13px !important;
}

.commit-bill-btn{
  background-color: #F44;
  border-color: #F44;
  position: relative;
  top: -1px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  margin: 0;
  margin-left: 10px;
  width: 35%;
  height: 51px;
  line-height: 50px;
  border-radius: 0;
  font-size: 14px;
  color: #fff;
  border: 0;
  float:right;
}

.express-panel{
  padding: 15px 10px;
  background-color: #fff;
  line-height: 22px;
  min-height: 22px;
  position: relative;
}

.express-panel::before{
  content: '';
  position: absolute;
  top: 18px;
  left: 10px;
  width: 18px;
  height: 18px;
  background-image: url(https://b.yzcdn.cn/v2/image/wap/address/site2@2x.png);
  background-size: 18px 18px;
}

.express-panel.express-panel-edit .express-detail{
  margin-right: 18px;
  margin: 0 0 0 25px;
}

.clearfix{
  zoom: 1;
}

.express-panel .name,{
  font-size: 14px;
  color: #333;
  float: left;
  width: 160px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.express-panel .tel{
  float: right;
}

.express-panel .name, .express-panel .tel{
  font-size: 14px;
  color: #333;
}

.clearfix:after{
  display: table;
  clear: both;
}

.express-panel .address-detail{
  margin-top: 2px;
  font-size: 12px;
  line-height: 16px;
  color: #666;
}

.express{
  padding: 0;
}

.express::after{
  content: '';
  height: 2px;
  background-image: url(../images/border_order_top.png);
  background-size: 34px 2px;
  display: border-box;
}

.express-detail .a .li{
  width: 100%;
  display: inline-block;
}

.expresse{
  width: 34px;
  height: 2px;
  display: flex;
  float: left;
}

.order_top_image{
  height:2px;
  width:110%;
  overflow:hidden;
  padding:0;
  position:relative;
  line-height:2px;
}

.blank-block{
  height: 2px;
  width: 100%;
}

.mask-layer-control{
  overflow: hidden;
  position: fixed;
  z-index: 1000;
  left: 0px;
  right: 0px;
  bottom: 0px;
  visibility: visible;
  opacity: 1;
  background: white;
}

.mask-layer-control .header{
  padding: 10px 0;
  position: relative;
  line-height: 30px;
  font-size: 16px;
  text-align: center;
  border-bottom: 1px solid #e5e5e5;
  color: #333;
}

.mask-layer-control .close{
  position: absolute;
  right: 6px;
  top: 5px;
  padding: 10px;
  height: 22px;
  width: 22px;
}

.mask-layer-control .list{
  margin: 0;
  padding: 0;
  padding-left: 10px;
  list-style: none;
  font-size: 14px;
  box-sizing: border-box;
}

.mask-layer-control .list .ul{
  max-height: 240px;
  overflow-y: scroll;
  background-color: #fff;
}

.mask-layer-control .list .item{
  cursor: pointer;
  display: block;
  position: relative;
  line-height: 1.5;
  border: 0;
  padding: 5px 20px 5px 30px;
  border-bottom: 1px solid #e5e5e5;
}

.mask-layer-control .check-img{
  position: absolute;
  left: 2px;
  top: 11px;
  width: 22px;
  height: 22px;
}

.mask-layer-control .list .item .single{
  line-height: 30px;
  height: inherit;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.mask-layer-control .footer{
  position: relative;
  padding: 20px 10px;
  padding-left: 0;
  margin: 0;
  margin-left: 10px;
  min-height:unset;
}

.btn-green{
  background-color: #06bf04;
  border-color: #03b401;
  border: 0;
  text-align: center;
  width: 100%;
  padding: 11px 10px;
  font-size: 16px;
  line-height: 16px;
  border-radius: 4px;
  box-sizing: border-box;
  color: #fff;
  margin:0;
}

.mask-layer-control .check-false{
  border:1px solid #999;
  border-radius: 40px;
  width: 16px;
  height:16px;
}

.empty-address-tip{
  position: relative;
  padding: 16px 10px 16px 60px;
  line-height: 40px;
  font-size: 14px;
  color: #333;
}

#noaddress .a{
  display: block;
  width: 100%;
}

#noaddress .a::after{
  content: '>';
  font-size: 20px;
  color:#999;
  float: right;
}

#noaddress image{
  position: absolute;
  left:10px;
  width: 40px;
  height: 40px;
}

.mask-layer-control .none-list{
  padding: 30px;
  text-align: center;
}

.name-card .gift {
  position: absolute;
  z-index: 2;
  width: 16px;
}

.right-col .span.s{
  text-decoration: line-through;
}

.footer{
  margin: 0;
}

.dividing-line {
  width: 100%;
  height: 10px;
  background: #f5f5f5;
}

.item-options[data-flag="shipping_time"] #shipping_time {
  font-size: 14px;
  float: right;
}

.item-options[data-flag="shipping_time"] .time {
  color: red;
  margin-right: 0px;
}

.item-options[data-flag="shipping_time"] .update, .delete {
  color: #0689e1;
  margin-left: 4px;
}

.item-options[data-flag="shipping_time"] .update{
  margin-right: 0;
}

.mask-layer-control .date-list {
  padding-bottom: 6px;
  height: 250px;
  overflow: auto;
}

.mask-layer-control .date-list .li {
  width: 30vw;
  height: 34px;
  background-color: #f7f7f7;
  border-radius: 4px;
  font-size: 13px;
  line-height: 34px;
  text-align: center;
  margin-left: 2.5vw;
  margin-top: 6px;
  float: left;
}

.mask-layer-control .date-list .li.active {
  background-color: #03b401;
  color: #fff;
}

.presell_radio {
  text-align: right;
}

.presell_radio .label {
  margin-right: 10px;
  display: inline-block;
}

.presell_radio .label .radio-common {
  position: relative;
  width: 13px !important;
  height: 13px !important;
  border: 1px solid #CCC;
  border-radius: 10px;
  margin-right: 5px;
  display: inline-block;
  vertical-align: middle;
}

.presell_radio .label .radio-common view{
  position: absolute;
  width: 9px;
  height: 9px;
  border-radius: 6px;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}

.presell_radio .label .radio-common.select view{
  background: #FF4444;
}