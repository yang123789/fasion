page {
  background: #eee;
}

.bro-spare {
  margin: 40px 0 0 0;
  border: 0;
  padding: 30px 15px 30px 15px;
  background: #333;
  margin-top: 0px;
  box-shadow: 2px 2px 2px rgb(220,220,220);
  overflow: hidden;
}

.tip_head {
  width: 56%;
  margin: auto;
  text-align: center;
}

.bro-spare .p.tip-txt {
  margin-bottom: 0;
  font-size: 16px;
  color: #fff;
  line-height: 25px;
}

.icon-money {
  width: 17px;
  height: 21.76px;
  margin: 0 8px;
  vertical-align: top;
  display:inline-block;
}

.bro-spare .number-big {
  display: block;
  margin: 0;
  font-size: 40px;
  color: #fff;
  
}

.bro-spare .number-big {
  font-size: 30px;
}

.tip_bottom {
  border-top: 1px solid #ffffff;
  margin-top: 20px;
}

.bro-spare .field-2 {
  margin: 0;
  width: 33.33%;
  text-align: center;
  padding-top: 25px;
  font-size: 14px;
  color: #fff;
}

.tip_bottom .lf {
  float: left;
}

.tip_bottom .rg {
  float: right;
}

.big {
  font-size: larger;
}

.promoter-box {
  background: #fff;
}

.xiajip {
  padding: 8px;
  margin-bottom: 0;
  border-bottom: 1px solid #eee;
}

.xiajispanleft {
  font-size: 14px;
  padding: 3px;
  line-height: 20px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  color: #000000;
}

.module-icon {
  display: inline-block;
  vertical-align: top;
  margin-right: 3px;
  width: 22px;
  height: 22px;
}

.xiajispanright {
  float: right;
  line-height: 28px;
  margin-right: 19px;
  color: #000000;
}

.maneylist {
  margin: 0 0 0 15px;
  padding: 0;
  list-style: none;
}

.maneylist-li {
  position: relative;
  padding: 10px 0 10px 15px;
  background: #fff;
  border-bottom: 1px solid #eee;
}

.maneylist-li:first-child {
  border-top: 0;
}

.maneylist-li .a {
  position: relative;
  display: block;
  color: #333;
  font-size: 12px;
}

.maneylist-li .a:before, .maneylist-li .a:after {
  content: ".";
  height: 0;
  display: block;
  visibility: hidden;
  clear: both;
}

.maneylist-li .text {
  font-size: 12px;
  vertical-align: middle;
  color: #000000;
}

.maneylist-li .money-number {
  float: right;
  margin: 0px;
  margin-right: 30px;
  padding: 0 5px;
  border-radius: 10px;
  height: 20px;
  line-height: 20px;
  color: orange;
  font-size: 14px;
  vertical-align: middle;
}

.arrow {
  display: inline-block;
  width: 6px;
  height: 11px;
}

.maneylist .maneylist-li .arrow {
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -5px;
}

.shareholder {
  display: inline-block;
}

.blank {
  height: 10px;
  background: #eee;
}

.nav_two {
  background: #fff;
}

.xiajip {
  padding: 8px;
  margin-bottom: 0;
  border-bottom: 1px solid #eee;
}

.xiajispanleft {
  font-size: 14px;
  padding: 3px;
  line-height: 20px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  color: #000000;
  margin-left:18px;
}

.gd .i {
  display: inline-block;
  vertical-align: top;
  margin-right: 3px;
  width: 22px;
  height: 22px;
}

.bro-extract-btn {
  background: #eee;
  padding: 20px 15px;
  margin-top: 0;
}

.button.red {
  background: #333;
  border: 1px solid #333;
}

.button {
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  width: 100%;
  padding: 0;
  border-radius: 3px;
  font-family: "微软雅黑","Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
  font-weight: normal;
  margin: 0 0 1.25rem;
  position: relative;
  text-decoration: none;
  text-align: center;
  -webkit-appearance: none;
  display: inline-block;
  color: white;
  transition: background-color 300ms ease-out;
}